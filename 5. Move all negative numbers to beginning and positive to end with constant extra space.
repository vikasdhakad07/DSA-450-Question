import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] arr=new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		dutch(arr,n);
	}
	public static void dutch(int arr[],int n){
	    int l=0,h=n-1,temp;
	    while(l<=h){
	        if(arr[l]<0){
	            l++;
	        }else if(arr[l]>0){
	            temp=arr[l];
	            arr[l]=arr[h];
	            arr[h]=temp;
	            h--;
	        }
	    }
	    for(int i=0;i<n;i++){
	       System.out.println(arr[i]);
	    }
	}
}
