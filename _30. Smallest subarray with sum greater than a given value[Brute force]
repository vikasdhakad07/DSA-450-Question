import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int x=sc.nextInt();
		int[] arr=new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		subArray(arr,n,x);
	}
	public static void subArray(int arr[],int n,int x){
	    int min=Integer.MAX_VALUE;
	    for(int i=0;i<n;i++){
	        int sum=0;
	        for(int j=i;j<n;j++){
	            sum=sum+arr[j];
	            if(sum>x){
	                min=Math.min(min,j-i+1);
	            }
	        }
	    }
	    System.out.println(min);
	}
}
