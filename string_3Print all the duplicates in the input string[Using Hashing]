import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		int count[]=new int[256];
		for(int i=0;i<s.length();i++){
		    count[s.charAt(i)]++;
		}
		for(int i=0;i<256;i++){
		    if(count[i]>1){
		        System.out.println((char)(i)+" count="+count[i]);
		    }
		}
	}
}
